@model BibliotecaWebApplication.Models.Ejemplar

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del Ejemplar</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Publicacion)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Publicacion.PublicacionId)</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Estante)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Estante.EstanteId)</dd>
    </dl>
</div>
<br />
<div>
    <a asp-action="Edit" asp-route-id="@Model?.EjemplarId" class="btn btn-edit"><i class="fas fa-edit"></i> Editar</a>
    <a asp-action="Index" class="btn btn-back"><i class="fas fa-arrow-left"></i> Volver</a>
</div>
